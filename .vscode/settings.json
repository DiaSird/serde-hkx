{
  "files.associations": {
    "*.s": "asm-intel-x86-generic"
  },
  "japanese-proofreading.textlint.かっこ類と隣接する文字の間のスペースの有無": false,
  "json.schemas": [
    {
      "fileMatch": ["assets/classes/*.json"],
      "url": "./assets/havok_class_schema.json"
    }
  ],
  "markdownlint.config": {
    "default": true,
    "MD024": false,
    "MD025": false,
    "MD033": false,
    "MD034": false, // OFF must use `<>`: Because the code must not be enclosed in `<>` for range visualization in github.
    "no-hard-tabs": false
  },
  "rust-analyzer.check.command": "clippy"
}
